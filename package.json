{
  "name": "vega-editor",
  "version": "0.1.0",
  "homepage": "https://boopathi.in/vega-editor",
  "devDependencies": {
    "autoprefixer": "6.4.1",
    "awesome-typescript-loader": "3.4.1",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^0.2.1",
    "babel-preset-react-optimize": "^1.0.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "chokidar": "^1.2.0",
    "connect-history-api-fallback": "1.4.0",
    "copy-webpack-plugin": "^4.2.0",
    "cross-spawn": "4.0.0",
    "css-loader": "0.28.7",
    "detect-port": "1.0.0",
    "dotenv": "^2.0.0",
    "eslint": "3.5.0",
    "eslint-config-react-app": "^0.2.1",
    "eslint-loader": "1.8.0",
    "eslint-plugin-flowtype": "2.18.1",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.11.2",
    "filesize": "3.5.11",
    "find-cache-dir": "1.0.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.30.1",
    "http-proxy-middleware": "0.17.4",
    "jest": "15.1.1",
    "json-loader": "0.5.7",
    "path-exists": "2.1.0",
    "postcss-loader": "^1.0.0",
    "promise": "8.0.1",
    "react-dev-utils": "0.2.1",
    "recursive-readdir": "2.2.1",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.3",
    "strip-ansi": "3.0.1",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "ajv": "^5.5.1",
    "d3-request": "1.0.5",
    "deep-equal": "^1.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.4.1",
    "react-monaco-editor": "^0.13.0",
    "react-portal": "^3.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "react-split-pane": "^0.1.74",
    "redux": "^3.7.2",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.2.0",
    "vega": "github:vega/vega#v3.0.8",
    "vega-datasets": "^1.11.0",
    "vega-lite": "github:vega/vega-lite#v2.0.4",
    "vega-schema-url-parser": "^1.0.0",
    "vega-tooltip": "^0.4.4"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "generate-example-images": "./scripts/generate-example-images.sh",
    "test": "node scripts/test.js --env=jsdom",
    "postinstall": "npm run vendor",
    "vendor": "./scripts/vendor.sh",
    "lint": "eslint src"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "env": {
      "production": {
        "presets": [
          "react-optimize"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app"
    ],
    "rules": {
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error",
            "info"
          ]
        }
      ],
      "space-before-function-paren": [
        "warn",
        "never"
      ],
      "object-curly-spacing": [
        "warn",
        "never"
      ],
      "quotes": [
        "error",
        "single"
      ]
    }
  }
}
